{
	"name": "@parser-generator/cli",
	"version": "0.0.1-alpha.0",
	"scripts": {
		"compile": "babel --extensions \".ts\" --copy-files --root-mode upward",
		"prebuild": "rimraf rm lib",
		"build": "tsc --emitDeclarationOnly && yarn run compile src --out-dir lib",
		"postbuild": "rimraf rm lib/__tests__",
		"pretest": "yarn run compile src --out-dir lib",
		"test": "nyc mocha lib/__tests__/**/*.test.js",
		"posttest": "nyc report --reporter=json"
	},
	"devDependencies": {
		"@types/getopts": "^2.1.0",
		"@types/html-webpack-plugin": "^3.2.1",
		"@types/shelljs": "^0.8.6",
		"@types/webpack": "^4.41.0",
		"shelljs": "^0.8.3",
		"vis-network": "^6.5.0",
		"webpack": "^4.41.5",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"@light0x00/shim": "^0.0.2",
		"@parser-generator/core": "^0.0.1-alpha.0",
		"@parser-generator/grammar-interpreter": "^0.0.1-alpha.0",
		"getopts": "^2.2.5",
		"html-webpack-plugin": "^3.2.0",
		"lodash-es": "^4.17.15",
		"vis-network": "^6.5.0",
		"webpack": "^4.41.5"
	},
	"bin": {
		"pgen": "lib/index.js"
	},
	"main": "lib/index.js",
	"files": [
		"lib"
	],
	"contributors": [
		"Chen Yang <light0x00@163.com> (https://github.com/light0x00)"
	],
	"description": "A Parser Generator that supports LL,SLR,LR1,LALR",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/light0x00/parser-generator.git"
	},
	"homepage": "https://github.com/light0x00/parser-generator",
	"keywords": [
		"Parser",
		"LL",
		"LR",
		"SLR",
		"LR1",
		"LALR"
	],
	"publishConfig": {
		"access": "public"
	}
}
